{% extends 'base.html.twig' %}

{% block title %}Datos index{% endblock %}

{% block body %}
    <h1>Datos index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>WindSpeed</th>
                <th>SolarIrradiance</th>
                <th>Location</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for dato in datos %}
            <tr>
                <td>{{ dato.id }}</td>
                <td>{{ dato.WindSpeed }}</td>
                <td>{{ dato.SolarIrradiance }}</td>
                <td>{{ dato.Location }}</td>
                <td>
                    <a href="{{ path('app_datos_show', {'id': dato.id}) }}">show</a>
                    <a href="{{ path('app_datos_edit', {'id': dato.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_datos_new') }}">Create new</a>
{% endblock %}
